{
  "presets": [
    [
      "env",
      {
        "browsers": [
          "last 2 versions",
          "IE >= 11"
        ],
        "exclude": [
          "transform-es2015-for-of"
        ],
        "loose": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-object-assign",
    "transform-for-of-as-array",
    "external-helpers"
  ]
}
