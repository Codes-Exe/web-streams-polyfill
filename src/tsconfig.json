{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [
      "es5",
      "es2015.promise"
    ],
    "strict": true,
    "stripInternal": true,
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "outDir": "../lib",
    "baseUrl": ".",
    "paths": {
      "@@target/*": [
        "./target/es5/*"
      ]
    }
  }
}
